<ol class="results">
  {{#answers}}
    <li>
      <span class="text">
        <span class="nominal">{{nominal}}:</span>
        {{text}}
      </span>
      <span class="count" data-count="{{counter}}">{{counter}}</span>
      <span class="percent">{{percent}}%</span>
    </li>
  {{/answers}}
</ol>
